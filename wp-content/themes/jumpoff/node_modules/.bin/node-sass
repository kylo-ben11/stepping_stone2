#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../node-sass/bin/node-sass" "$@"
  ret=$?
else 
  node  "$basedir/../node-sass/bin/node-sass" "$@"
  ret=$?
fi
exit $ret
